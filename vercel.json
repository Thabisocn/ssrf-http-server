{
  "version": 2,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [ "src/test.txt" ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/app.ts"
    }
  ]
}
